{
    "name": "react-photo-grid",
    "version": "1.4.4",
    "description":
        "Show images in a grid like facebook does. Useful to show a preview of main images when many are uploaded.",
    "main": "lib/index.js",
    "modules": "es/index.js",
    "files": ["es", "lib", "umd"],
    "scripts": {
        "build": "npx nwb build-react-component",
        "clean": "npx nwb clean-module && npx clean-demo",
        "start": "npx nwb serve-react-demo",
        "lint": "eslint src"
    },
    "keywords": [
        "react-photo-grid",
        "react-image-grid",
        "react-component",
        "reactjs",
        "react",
        "resizable image grid"
    ],
    "browserify": {
        "transform": ["reactify", "node-lessify"]
    },
    "author": "Mukesh Soni <mukeshsoni@gmail.com>",
    "repository": "https://github.com/mukeshsoni/react-photo-grid",
    "license": "MIT",
    "devDependencies": {
        "babel-preset-latest": "6.24.1",
        "babel-preset-react": "6.24.1",
        "chai": "^3.0.0",
        "dirty-chai": "^1.2.2",
        "eslint": "0.23.0",
        "nwb": "0.24.x",
        "react": "^15.x",
        "react-dom": "^15.3.2",
        "regenerator-runtime": "0.10.5"
    },
    "dependencies": {
        "lodash.throttle": "4.1.1"
    },
    "peerDependencies": {
        "react": ">=0.14.0 || ^15.0.0",
        "react-dom": ">=0.14.0 || ^15.0.0"
    }
}
