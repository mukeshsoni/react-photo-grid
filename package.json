{
  "name": "react-photo-grid",
  "version": "1.4.4",
  "description": "Show images in a grid like facebook does. Useful to show a preview of main images when many are uploaded.",
  "main": "lib/index.js",
  "modules": "es/index.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "npx nwb build-react-component",
    "clean": "npx nwb clean-module && npx clean-demo",
    "start": "npx nwb serve-react-demo",
    "deploy": "gh-pages -d demo/dist"
  },
  "keywords": [
    "react-photo-grid",
    "react-image-grid",
    "react-component",
    "reactjs",
    "react",
    "resizable image grid"
  ],
  "author": "Mukesh Soni <mukeshsoni@gmail.com>",
  "repository": "https://github.com/mukeshsoni/react-photo-grid",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "gh-pages": "^2.2.0",
    "nwb": "0.24.x",
    "react": "^16.x",
    "react-dom": "^16.13.1",
    "react-syntax-highlighter": "^12.2.1",
    "regenerator-runtime": "0.13.5"
  },
  "dependencies": {
    "lodash.throttle": "4.1.1"
  },
  "peerDependencies": {
    "react": ">=0.14.0 || ^15.0.0",
    "react-dom": ">=0.14.0 || ^15.0.0"
  }
